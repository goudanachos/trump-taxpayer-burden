<header style="background: var(--color-bg-command); border-bottom: var(--border-classification); padding: var(--space-lg) 0;">
    <div class="max-w-5xl mx-auto px-4 flex items-center justify-between">
        <!-- Left: Federal Identity with Eagle Silhouette -->
        <div class="flex items-center" style="gap: var(--space-md);">
            <!-- Federal Authority Eagle Seal - Maximum Precision -->
            <div style="width: 44px; height: 44px; opacity: 1; filter: drop-shadow(0 2px 4px rgba(0,0,0,0.3));">
                <svg width="44" height="44" viewBox="0 0 44 44" fill="none" xmlns="http://www.w3.org/2000/svg" style="shape-rendering: geometricPrecision;">
                    <!-- Outer Authority Ring -->
                    <circle cx="22" cy="22" r="21" stroke="var(--color-text-inverse)" stroke-width="2" fill="transparent"/>
                    <circle cx="22" cy="22" r="18.5" stroke="var(--color-text-inverse)" stroke-width="0.5" fill="transparent" opacity="0.6"/>
                    
                    <!-- Eagle Silhouette - Brutalist Precision -->
                    <g transform="translate(22,22)">
                        <!-- Eagle Head - Angular Authority -->
                        <path d="M0,-12 L4,-10 L6,-8 L5,-6 L4,-5 L2,-6 L0,-8 L-2,-6 L-4,-5 L-5,-6 L-6,-8 L-4,-10 L0,-12 Z" fill="var(--color-text-inverse)"/>
                        
                        <!-- Eagle Beak - Sharp Command -->
                        <path d="M6,-8 L9,-7 L6,-6 Z" fill="var(--color-text-inverse)"/>
                        
                        <!-- Eagle Eye - Surveillance Dot -->
                        <circle cx="3" cy="-7" r="0.8" fill="var(--color-bg-command)"/>
                        
                        <!-- Left Wing - Spread for Maximum Coverage -->
                        <path d="M-10,-4 L-15,-6 L-18,-2 L-16,2 L-12,1 L-8,-2 L-10,-4 Z" fill="var(--color-text-inverse)"/>
                        
                        <!-- Right Wing - Mirror Precision -->
                        <path d="M10,-4 L15,-6 L18,-2 L16,2 L12,1 L8,-2 L10,-4 Z" fill="var(--color-text-inverse)"/>
                        
                        <!-- Eagle Body - Central Authority -->
                        <ellipse cx="0" cy="0" rx="3" ry="5" fill="var(--color-text-inverse)"/>
                        
                        <!-- Left Talon - Grip -->
                        <path d="M-3,6 L-5,8 L-4,10 L-2,8 Z" fill="var(--color-text-inverse)"/>
                        <path d="M-2,6 L-4,8 L-3,10 L-1,8 Z" fill="var(--color-text-inverse)"/>
                        
                        <!-- Right Talon - Mirror Grip -->
                        <path d="M3,6 L5,8 L4,10 L2,8 Z" fill="var(--color-text-inverse)"/>
                        <path d="M2,6 L4,8 L3,10 L1,8 Z" fill="var(--color-text-inverse)"/>
                        
                        <!-- Central Shield - Protection -->
                        <rect x="-2" y="-2" width="4" height="5" fill="var(--color-bg-command)" stroke="var(--color-text-inverse)" stroke-width="0.5"/>
                        
                        <!-- Authority Star - Federal Power -->
                        <polygon points="0,-16 1,-14.5 2.5,-14.5 1.3,-13.5 1.8,-12 0,-13 -1.8,-12 -1.3,-13.5 -2.5,-14.5 -1,-14.5" fill="var(--color-text-inverse)"/>
                        
                        <!-- Micro Details - Technical Precision -->
                        <circle cx="0" cy="-1" r="0.5" fill="var(--color-bg-command)" opacity="0.8"/>
                        <rect x="-1" y="3" width="2" height="1" fill="var(--color-bg-command)" opacity="0.6"/>
                    </g>
                    
                    <!-- Outer Text Ring - Micro Typography -->
                    <path id="circle-text" d="M 22,6 A 16,16 0 0,1 38,22 A 16,16 0 0,1 22,38 A 16,16 0 0,1 6,22 A 16,16 0 0,1 22,6" fill="none" stroke="none"/>
                    <text font-family="monospace" font-size="3" fill="var(--color-text-inverse)" opacity="0.4">
                        <textPath href="#circle-text" startOffset="0%">FISCAL•OVERSIGHT•AUTHORITY</textPath>
                    </text>
                </svg>
            </div>
            
            <!-- Site Name - Government Authority Style -->
            <h1 class="authority-font" style="color: var(--color-text-inverse); font-size: var(--text-command); margin: 0;">
                TRUMP TAXPAYER BURDEN
            </h1>
        </div>
        
        <!-- Right: Command Navigation -->
        <nav class="flex items-center" style="gap: var(--space-lg);" aria-label="Command navigation">
            <a href="/" class="standard-font" style="color: var(--color-text-inverse); font-size: var(--text-detail); text-decoration: none; padding: var(--space-xs) var(--space-sm); border: var(--border-width-thin) solid transparent; text-transform: uppercase; letter-spacing: 0.05em; transition: all 0.2s ease;"
               onmouseover="this.style.backgroundColor='var(--color-text-inverse)'; this.style.color='var(--color-bg-command)'; this.style.borderColor='var(--color-text-inverse)';"
               onmouseout="this.style.backgroundColor='transparent'; this.style.color='var(--color-text-inverse)'; this.style.borderColor='transparent';">
                STATUS
            </a>
            <a href="/archive" class="standard-font" style="color: var(--color-text-inverse); font-size: var(--text-detail); text-decoration: none; padding: var(--space-xs) var(--space-sm); border: var(--border-width-thin) solid transparent; text-transform: uppercase; letter-spacing: 0.05em; transition: all 0.2s ease;"
               onmouseover="this.style.backgroundColor='var(--color-text-inverse)'; this.style.color='var(--color-bg-command)'; this.style.borderColor='var(--color-text-inverse)';"
               onmouseout="this.style.backgroundColor='transparent'; this.style.color='var(--color-text-inverse)'; this.style.borderColor='transparent';">
                ARCHIVE
            </a>
            <a href="/about" class="standard-font" style="color: var(--color-text-inverse); font-size: var(--text-detail); text-decoration: none; padding: var(--space-xs) var(--space-sm); border: var(--border-width-thin) solid transparent; text-transform: uppercase; letter-spacing: 0.05em; transition: all 0.2s ease;"
               onmouseover="this.style.backgroundColor='var(--color-text-inverse)'; this.style.color='var(--color-bg-command)'; this.style.borderColor='var(--color-text-inverse)';"
               onmouseout="this.style.backgroundColor='transparent'; this.style.color='var(--color-text-inverse)'; this.style.borderColor='transparent';">
                INTEL
            </a>
        </nav>
    </div>
</header>